
/ {
	host {
		#address-cells = <1>;
		#size-cells = <1>;

		model = "exynos4210";
		device_type = "host";

		cpus {
			cpu0 {
				device_type = "cpu";
				model = "Cortex-A9";
			};

			cpu1 {
				device_type = "cpu";
				model = "Cortex-A9";
			};
		};

		memory {
			/* NOTE: By default QEMU will only proivde 128 MB 
			 * unless specified by "-m" option. If you have
			 * X MB then we will be able to access only first
			 * X MB starting at physical_addr.
			 */
			physical_addr = <0x40000000>;
			physical_size = <0x10000000>; /* 256 MB */
		};

		scu { /* Snoop Control Unit */
			device_type = "scu";
			compatible = "arm,a9mpcore-priv";
			reg = <0x10500000 0x1000>;
		};

		pmu { /* Performance Monitorig Unit */
			device_type = "pmu";
			compatible = "arm,a9mpcore-priv";
			reg = <0x10020000 0x10000>;
		};

		gic {
			device_type = "pic";
			compatible = "arm,cortex-a9-gic";
			reg = <0x10490000 0x1000 0x10480000 0x100>;
			irq_start = <29>;
		};

		mct {
			device_type = "timer";
			compatible = "samsung,exynos4210-mct";
			reg = <0x10050000 0x1000>;
			clock-rate = <24000000>;
			irq = <89 74 80>;
		};

		sfrregion {
			#address-cells = <1>;
			#size-cells = <1>;

			RTC0: s3c-rtc {
				device_type = "rtc";
				compatible = "samsung,s3c6410-rtc";
				reg = <0x10070000 0x100>;
				clock-rate = <1>;
				irq = <76 77>;
			};

			SERIAL0: uart@13800000 {
				device_type = "serial";
				compatible = "samsung,exynos4210-uart";
				reg = <0x13800000 0x100>;
				clock-rate = <100000000>;
				baudrate = <115200>;
				irq = <84>;
			};

			SERIAL1: uart@13810000 {
				device_type = "serial";
				compatible = "samsung,exynos4210-uart";
				reg = <0x13810000 0x100>;
				clock-rate = <100000000>;
				baudrate = <115200>;
				irq = <85>;
			};

			SERIAL2: uart@13820000 {
				device_type = "serial";
				compatible = "samsung,exynos4210-uart";
				reg = <0x13820000 0x100>;
				clock-rate = <100000000>;
				baudrate = <115200>;
				irq = <86>;
			};

			SERIAL3: uart@13830000 {
				device_type = "serial";
				compatible = "samsung,exynos4210-uart";
				reg = <0x13830000 0x100>;
				clock-rate = <100000000>;
				baudrate = <115200>;
				irq = <87>;
			};

			SERIAL4: uart@13840000 {
				device_type = "serial";
				compatible = "samsung,exynos4210-uart";
				reg = <0x13840000 0x100>;
				clock-rate = <100000000>;
				baudrate = <115200>;
				irq = <88>;
			};

			timer0 {
				device_type = "timer";
				compatible = "samsung,exynos4210-PWMtimer";
				reg = <0x139d0000 0x1000>;
				irq = <69 70 71 72 73>;
			};
		};
	};
};
